{
    "name": "MerchAtScale",
    "version": "0.1.1",
    "private": true,
    "type": "module",
    "dependencies": {
        "@adobe/aio-lib-ims": "^7.0.2",
        "@adobe/aio-lib-state": "^5.1.0",
        "@adobe/aio-sdk": "^6",
        "@adobe/exc-app": "^1.3.0",
        "cloudevents": "^4.0.2",
        "regenerator-runtime": "^0.13.5",
        "sinon": "^21.0.0",
        "uuid": "^8.0.0"
    },
    "devDependencies": {
        "c8": "^10.1.3",
        "chai": "^4.3.6",
        "eslint": "^8",
        "esm": "^3.2.25",
        "mocha": "^10.2.0",
        "nock": "^13.0.0",
        "nodemon": "^3.1.10"
    },
    "scripts": {
        "test": "c8 --check-coverage --lines=99 --functions=99 --branches=99 --statements=99 mocha --recursive ./test",
        "test:watch": "nodemon --watch src --watch test --ext js,json --exec \"mocha --recursive ./test\"",
        "test:file": "nodemon --watch src --watch test --ext js,json --exec \"mocha --recursive ./test --grep \\\"$npm_config_grep\\\"\"",
        "test:coverage": "c8 --reporter=lcov --reporter=text-summary --check-coverage --lines=99 --functions=99 --branches=99 --statements=99 mocha --recursive ./test",
        "lint": "eslint --ignore-pattern web-src --no-error-on-unmatched-pattern test src actions",
        "lint:fix": "npm run lint -- --fix"
    },
    "engines": {
        "node": ">=22"
    }
}
